{
  "version": 2,
  "builds": [
    { "src": "api/index.ts", "use": "@now/node" },
    { "src": "www/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)",
      "dest": "/www/$1",
      "headers": { "x-request-path": "$1" } }
  ]
}
