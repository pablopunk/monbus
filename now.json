{
  "version": 2,
  "name": "monbus",
  "alias": "raxo",
  "builds": [
    { "src": "api/index.js", "use": "now-micro" },
    { "src": "www/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)",
      "dest": "/www/$1",
      "headers": { "x-request-path": "$1" } }
  ]
}
