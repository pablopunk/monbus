{
  "version": 2,
  "name": "monbus",
  "alias": "raxo",
  "builds": [
    { "src": "api/index.js",
      "use": "@now/node-server",
      "config": { "maxLambdaSize": "40mb" } },
    { "src": "www/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)",
      "dest": "/www/$1",
      "headers": { "x-request-path": "$1" } }
  ]
}
